{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "api",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}/api/health",
      "serviceActions": {
        "api": "StartDebugging",
        "api-tests": "DoNotStart",
        "mssql-db": "StartWithoutDebugging"
      }
    }
  }
}